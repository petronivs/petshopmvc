@model IEnumerable<PetShopTest.Models.Pets>

@{
    ViewData["Title"] = "PetShopMain";
}

<h2>Pet Shop</h2>

<table>
    @foreach (var item in Model)
    {
    <tr>
        <td>
            @Html.DisplayFor(ModelItem => item.Id)
        </td>
        <td>
            @Html.DisplayFor(ModelItem => item.Name)
        </td>
    </tr>
    }
</table>